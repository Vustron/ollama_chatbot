{
  "name": "nextjs_template_python_backend",
  "version": "1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && .venv/Scripts/activate && python -m uvicorn api.index:app --reload",
    "next-dev": "next dev --turbo",
    "dev": "concurrently \"bun run next-dev\" \"bun run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "bunx @biomejs/biome lint --write --unsafe src",
    "format": "bunx @biomejs/biome format --write src",
    "fmt:fix": "bun run lint && bun run format && bun run typecheck",
    "check-dep": "bunx upgradeps -v",
    "prod-test": "turbo build && turbo start",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "3.9.0",
    "@radix-ui/react-alert-dialog": "1.1.1",
    "@radix-ui/react-avatar": "1.1.0",
    "@radix-ui/react-checkbox": "1.1.1",
    "@radix-ui/react-dialog": "1.1.1",
    "@radix-ui/react-dropdown-menu": "2.1.1",
    "@radix-ui/react-label": "2.1.0",
    "@radix-ui/react-popover": "1.1.1",
    "@radix-ui/react-progress": "1.1.0",
    "@radix-ui/react-scroll-area": "1.1.0",
    "@radix-ui/react-slot": "1.1.0",
    "@t3-oss/env-nextjs": "0.11.1",
    "@tanstack/react-query": "5.53.1",
    "@tanstack/react-query-devtools": "5.53.1",
    "@tanstack/react-table": "8.19.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "cmdk": "1.0.0",
    "concurrently": "8.2.2",
    "dotenv": "16.4.5",
    "framer-motion": "11.3.31",
    "goober": "2.1.14",
    "isomorphic-dompurify": "2.15.0",
    "jiti": "1.21.6",
    "lucide-react": "0.437.0",
    "mini-svg-data-uri": "^1.4.4",
    "next": "14.2.7",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.53.0",
    "react-hot-toast": "2.4.1",
    "tailwind-merge": "2.5.2",
    "tailwindcss-animate": "1.0.7",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@next/bundle-analyzer": "14.2.7",
    "@types/node": "22.5.1",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/request-ip": "0.0.41",
    "autoprefixer": "10.4.20",
    "cross-env": "7.0.3",
    "husky": "^9.1.5",
    "postcss": "8.4.42",
    "tailwindcss": "3.4.10",
    "turbo": "2.1.1",
    "typescript": "5.5.4",
    "upgradeps": "2.0.6"
  },
  "packageManager": "bun@1.1.27",
  "trustedDependencies": ["@biomejs/biome"]
}
